{
  "name": "elronom-free-academy",
  "version": "0.2.0",
  "type": "module",
  "description": "Free MultiversX Trading Academy - Zero budget educational platform with modern React, TypeScript, and AI integration",
  "keywords": ["multiversx", "blockchain", "education", "trading", "crypto", "academy", "ai", "supabase", "vercel"],
  "author": "Pricop George <pricopgeorge@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gzeu/elronom-free-academy.git"
  },
  "homepage": "https://elronom-free-academy.vercel.app",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "analyze": "vite-bundle-analyzer",
    "deploy-preview": "vercel",
    "deploy-prod": "vercel --prod"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "@multiversx/sdk-core": "^13.8.0",
    "@multiversx/sdk-dapp": "^2.33.3",
    "@supabase/supabase-js": "^2.45.4",
    "lucide-react": "^0.445.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2",
    "framer-motion": "^11.5.6",
    "zustand": "^4.5.5",
    "react-hook-form": "^7.53.0",
    "@hookform/resolvers": "^3.9.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "@vitest/coverage-v8": "^2.1.1",
    "vite-bundle-analyzer": "^0.11.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}